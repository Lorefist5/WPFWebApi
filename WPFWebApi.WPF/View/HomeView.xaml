<UserControl
    x:Class="WPFWebApi.WPF.View.HomeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:common="clr-namespace:WPFWebApi.WPF.Common"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WPFWebApi.WPF.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <common:MainpageLayout Title="Home" Grid.Row="1">
        <StackPanel Orientation="Horizontal">
            <Button
                Margin="30"
                Padding="10"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Command="{Binding NavigateToUsers}"
                Content="Go to Users"
                Style="{StaticResource PButton}" />

        </StackPanel>
    </common:MainpageLayout>
</UserControl>
